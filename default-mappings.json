{
  "settings" : {
     "index":{
        "analysis":{
           "analyzer":{
              "analyzer_keyword":{
                 "tokenizer":"keyword",
                 "filter":"lowercase"
              }
           }
        }
     }
   },
  "mappings": {
    "_default_": {
      "dynamic_templates": [
        {
          "string_fields": {
            "match": "*",
            "match_mapping_type": "string",
            "mapping": {
              "index": true,
              "omit_norms": true,
              "type": "text"
            }
          }
        }
      ],
      "properties": {
        "id": {
          "type": "keyword",
          "index": true
        },
        "createdOn": {
          "type": "date"
        },
        "updatedOn": {
          "type": "date"
        },
        "name": {
          "type": "keyword",
          "index": true
        },
        "slug": {
          "type": "keyword",
          "index": true
        }
      }
    }
  }
}
